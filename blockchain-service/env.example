# Base RPC URL (required)
RPC_URL=https://mainnet.base.org

# Path to write/read clanker state JSON (default: ~/.obelisk-core/data/clanker_state.json)
STATE_FILE_PATH=

# Clanker V4 hook address – only pool inits with this hook are tracked
CLANKER_HOOK_ADDRESS=0xb429d62f8f3bFFb98CdB9569533eA23bF0Ba28CC

# GodMulticall contract (from base-swap-tracker) – one RPC for decimals + totalSupply for new pools
# Optional; default: 0xEAae97dd1220C19C49cadc04C0f7aC5866fcEA3d
GOD_MULTICALL_ADDRESS=

# Persist state every N seconds
PERSIST_INTERVAL_SEC=30

# Block polling interval in ms
BLOCK_POLL_MS=1000
